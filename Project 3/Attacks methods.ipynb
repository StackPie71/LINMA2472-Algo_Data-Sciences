   "metadata": {},
   "outputs": [],
   "source": [
    "# Lookup table is a brute-force attack\n",
    "from hashlib import md5\n",
    "\n",
    "def create_lookup_table(N):\n",
    "    table = {}\n",
    "    for id in range(10**N):\n",
    "        h = md5(str(id).encode()).hexdigest()\n",
    "        table[h] = id\n",
    "    return table"
   ]
